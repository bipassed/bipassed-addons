<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <HediffDef ParentName="ImplantHediffBase">
        <defName>HediffGalactorrheaImplant</defName>
        <label>galactorrhea implant</label>
        <labelNoun>a galactorrhea implant</labelNoun>
        <description>An installed galactorrhea implant.</description>
        <descriptionHyperlinks>
            <ThingDef>GalactorrheaImplant</ThingDef>
        </descriptionHyperlinks>
        <stages>
            <li>
                <partEfficiencyOffset>-0.1</partEfficiencyOffset>
            </li>
        </stages>
        <spawnThingOnRemoved>GalactorrheaImplant</spawnThingOnRemoved>
    </HediffDef>

    <ThingDef ParentName="BodyPartProstheticBase">
        <defName>GalactorrheaImplant</defName>
        <label>galactorrhea implant</label>
        <description>An invasive surgical implant on the brain that induces constant lactation, reduces brain capacity as the implant uses parts of the brain to redirect unused resources towards lactation.</description>
        <descriptionHyperlinks>
            <RecipeDef>InstallGalactorrheaImplant</RecipeDef>
        </descriptionHyperlinks>
        <thingSetMakerTags>
            <li>RewardStandardLowFreq</li>
        </thingSetMakerTags>
        <costList>
            <Steel>30</Steel>
            <ComponentIndustrial>2</ComponentIndustrial>
        </costList>
        <techHediffsTags>
            <li>Advanced</li>
        </techHediffsTags>
    </ThingDef>

    <RecipeDef ParentName="SurgeryInstallImplantBase">
        <defName>InstallGalactorrheaImplant</defName>
        <label>install galactorrhea implant</label>
        <description>Install a galactorrhea implant.</description>
        <descriptionHyperlinks>
            <ThingDef>GalactorrheaImplant</ThingDef>
            <HediffDef>HediffGalactorrheaImplant</HediffDef>
        </descriptionHyperlinks>
        <jobString>Installing galactorrhea implant.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>GalactorrheaImplant</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>GalactorrheaImplant</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Brain</li>
        </appliedOnFixedBodyParts>
        <addsHediff>HediffGalactorrheaImplant</addsHediff>
    </RecipeDef>

</Defs>